   "source": [
    "rf_importance = pd.DataFrame({\n",
    "    \"feature\": X.columns,\n",
    "    \"importance\": rf.feature_importances_\n",
    "})\n",
    "\n",
    "# Normalize to sum to 1\n",
    "rf_importance[\"weight\"] = rf_importance[\"importance\"] / rf_importance[\"importance\"].sum()\n",
    "rf_importance = rf_importance.sort_values(\"weight\", ascending=False)\n",
    "rf_importance\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 29,
   "id": "566b3329-3211-4f0e-a23e-11930f752d29",
   "metadata": {},
   "outputs": [],
   "source": [
    "def compute_penalties(row):\n",
    "    # Sleep: ideal ~7–9; penalty grows as you move away from 8\n",
    "    sleep_penalty = min(1.0, abs(row[\"sleep_hours\"] - 8) / 4)   # 0 if 8h, ~1 if 4h or 12h\n",
    "\n",
    "    # Screen time: ideal <= 4h\n",
    "    screen_penalty = min(1.0, max(0, row[\"screen_time_hours\"] - 4) / 6)  # 4h→0, 10h→~1\n",
    "\n",
    "    # Exercise: ideal >= 30 min\n",
    "    exercise_penalty = min(1.0, max(0, 30 - row[\"exercise_minutes\"]) / 30)  # 30+→0, 0→1\n",
    "\n",
    "    # Water: ideal ~2–3L\n",
    "    water_penalty = min(1.0, max(0, 2 - row[\"water_intake_liters\"]) / 1)    # <1L→1, 2L→0\n",
    "\n",
    "    # Meditation: ideal ~10–20 min, but softer\n",
    "    meditation_penalty = min(1.0, max(0, 10 - row[\"meditation_minutes\"]) / 10)\n",
    "\n",
    "    return {\n",
    "        \"sleep_penalty\": sleep_penalty,\n",
    "        \"screen_penalty\": screen_penalty,\n",
    "        \"exercise_penalty\": exercise_penalty,\n",
    "        \"water_penalty\": water_penalty,\n",
    "        \"meditation_penalty\": meditation_penalty\n",
    "    }\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 32,
   "id": "edc83bfd-15f5-4bbd-b2d1-a62c784bd992",
   "metadata": {
    "scrolled": true
   },
   "outputs": [
    {
     "data": {
      "text/plain": [
       "(10400, 7)"
      ]
     },
     "execution_count": 32,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "# Make a mapping from feature → weight\n",
    "weight_map = dict(zip(rf_importance[\"feature\"], rf_importance[\"weight\"]))\n",
    "\n",
    "def mindgene_score(row):\n",
    "    p = compute_penalties(row)\n",
    "\n",
    "    # map penalty keys to feature names\n",
    "    weighted_sum = (\n",
    "        weight_map[\"sleep_hours\"]          * p[\"sleep_penalty\"] +\n",
    "        weight_map[\"screen_time_hours\"]    * p[\"screen_penalty\"] +\n",
    "        weight_map[\"exercise_minutes\"]     * p[\"exercise_penalty\"] +\n",
    "        weight_map[\"water_intake_liters\"]  * p[\"water_penalty\"] +\n",
    "        weight_map[\"meditation_minutes\"]   * p[\"meditation_penalty\"]\n",
    "    )\n",
    "\n",
    "    # convert 0–1 → 0–100\n",
    "    score = round(100 * weighted_sum)\n",
    "    return score\n",
    "\n",
    "df[\"mindgene_score\"] = df.apply(mindgene_score, axis=1)\n",
    "df[[\"sleep_hours\",\"screen_time_hours\",\"exercise_minutes\",\n",
    "    \"water_intake_liters\",\"meditation_minutes\",\"mindgene_score\"]].head()\n",
    "len(df)\n",
    "df.shape\n"
   ]
  },
