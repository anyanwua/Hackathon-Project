   "source": [
    "import pandas as pd\n",
    "import numpy as np\n",
    "\n",
    "# ======================================================\n",
    "# 1. Load datasets (YOUR PATHS)\n",
    "# ======================================================\n",
    "holistic = pd.read_csv(r\"C:\\Users\\dhruv\\OneDrive\\Desktop\\Venture Track Project\\archive (1)\\holistic_health_lifestyle_dataset.csv\")\n",
    "screen = pd.read_csv(r\"C:\\Users\\dhruv\\OneDrive\\Desktop\\Venture Track Project\\archive (1)\\ScreenTime vs MentalWellness.csv\")\n",
    "\n",
    "# ======================================================\n",
    "# 2. Rename columns so both datasets match\n",
    "# ======================================================\n",
    "\n",
    "# --- Holistic dataset ---\n",
    "holistic = holistic.rename(columns={\n",
    "    \"Sleep_Hours\": \"sleep_hours\",\n",
    "    \"Hydration\": \"water_intake_liters\",\n",
    "    \"Physical_Activity\": \"exercise_minutes\",\n",
    "    \"Stress_Level\": \"stress_level\",\n",
    "    \"Mindfulness\": \"meditation_minutes\"\n",
    "})\n",
    "\n",
    "# Impute **screen time** for holistic dataset\n",
    "holistic[\"screen_time_hours\"] = np.random.uniform(3, 10, len(holistic))\n",
    "\n",
    "# --- Screen Time dataset ---\n",
    "screen = screen.rename(columns={\n",
    "    \"leisure_screen_hours\": \"screen_time_hours\",\n",
    "    \"sleep_hours\": \"sleep_hours\",\n",
    "    \"stress_level_0_10\": \"stress_level\",\n",
    "    \"exercise_minutes_per_week\": \"exercise_minutes\"\n",
    "})\n",
    "\n",
    "# Impute missing **water intake** and **meditation minutes**\n",
    "screen[\"water_intake_liters\"] = np.random.uniform(1.5, 3.5, len(screen))\n",
    "screen[\"meditation_minutes\"] = np.random.uniform(0, 20, len(screen))\n",
    "\n",
    "\n",
    "# ======================================================\n",
    "# 3. Select FINAL standardized columns\n",
    "# ======================================================\n",
    "final_columns = [\n",
    "    \"sleep_hours\",\n",
    "    \"screen_time_hours\",\n",
    "    \"exercise_minutes\",\n",
    "    \"water_intake_liters\",\n",
    "    \"stress_level\",\n",
    "    \"meditation_minutes\"\n",
    "]\n",
    "\n",
    "holistic_final = holistic[final_columns]\n",
    "screen_final   = screen[final_columns]\n",
    "\n",
    "# ======================================================\n",
    "# 4. Combine datasets\n",
    "# ======================================================\n",
    "df_combined = pd.concat([holistic_final, screen_final], ignore_index=True)\n",
    "\n",
    "# ======================================================\n",
    "# 5. Preview merged dataset\n",
    "# ======================================================\n",
    "print(\"Preview:\")\n",
    "print(df_combined.head())\n",
    "print(\"\\nFinal shape:\", df_combined.shape)\n",
    "\n",
    "# ======================================================\n",
    "# 6. Save final unified dataset\n",
    "# ======================================================\n",
    "output_path = r\"C:\\Users\\dhruv\\OneDrive\\Desktop\\Venture Track Project\\unified_lifestyle_dataset.csv\"\n",
    "df_combined.to_csv(output_path, index=False)\n",
    "\n",
    "print(f\"\\nUnified dataset saved to: {output_path}\")\n"
   ]
  },
